{
  "name": "anycicd",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "node ./bin/www",
    "dev": " nodemon ",
    "clean" : "rm -r -- dist",
    "build" : "yarn run clean ; pkg bin/www --config package.json "
  },
  "pkg": {
    "scripts": "bin/*.js",
    "assets": [
      "public/**/*",
      "logs/*",
      "exec/*"
    ],
    "targets": [
      "node16"
    ],
    "outputPath": "dist"

  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-rate-limit": "^6.3.0",
    "morgan": "~1.9.1",
    "rotating-file-stream": "^3.0.3",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "nodemon": "^2.0.15",
    "pkg": "^5.5.2"
  }
}
